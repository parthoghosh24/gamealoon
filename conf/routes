# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
#GET     /                           com.gamealoon.controllers.Application.index()
GET     /                           com.gamealoon.controllers.HomeController.index()
#GET   /game/:title                            com.gamealoon.controllers.GloonAPI.getGame(title:String)
GET   /game/                            com.gamealoon.controllers.GloonAPIController.getGames()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
